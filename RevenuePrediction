import streamlit as st
import pickie
import numpy as np
model = pickie.load(open('model.pickie','rb'))
st.title("Revenue Prediction")
x = st.number_input('Input Temperature')
if st.button('Predict'):
  x = np.array(x)
  x = x.reshape(-1,1)
  result = model.predict(x)
  st.caption('Revenue Prediction')
  st.success(result)
